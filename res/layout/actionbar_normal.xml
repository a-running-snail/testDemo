<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >

    <ImageView
        android:id="@+id/return_main"
        android:layout_width="7dp"
        android:layout_height="14dp"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:background="@drawable/return_to_main"
      />

    <ImageView
        android:id="@+id/logoImage"
        android:layout_width="40dp"
        android:layout_height="38dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="7dp"
        android:layout_marginTop="7dp"
        android:layout_toRightOf="@id/return_main"
        android:background="@drawable/ic_launcher"
        android:contentDescription="@string/app_name" />

    <TextView
        android:id="@+id/actionBarReturnText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="5dp"
        android:layout_toRightOf="@id/logoImage"
        android:singleLine="true"
        android:text="ActionBar的测试"
        android:textColor="@android:color/white"
        android:textSize="20sp" />

</RelativeLayout>